{
    "name": "@contantly-ui/repo",
    "version": "1.0.0",
    "description": "contactly frontend design system",
    "license": "MIT",
    "author": "Rick Chi",
    "module": "dist/index.js",
    "sideEffects": false,
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "homepage": "",
    "repository": "https://github.com/wenzhongchi/contactly-ui",
    "bugs": {
        "url": "https://github.com/wenzhongchi/contactly-ui/issues"
    },
    "scripts": {
        "build": "rm -rf dist && ttsc",
        "type-check": "tsc --noEmit",
        "lint": "eslint './src/**/*.{ts,tsx}'",
        "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
        "format": "prettier --write './src/**/*.{ts,tsx,js,md,json}'",
        "pre-push": "yarn lint && yarn type-check",
        "clear": "rm -rf dist && rm -rf build && rm -rf yarn.lock && rm -rf node_modules && yarn install",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -o .out",
        "build:icons": "./scripts/icons/generate.js -o src/icon/src src/assets/icons && yarn lint:fix && yarn prettier -w --loglevel=warn ./src/icons/src"
    },
    "husky": {
        "hooks": {
            "pre-push": "yarn pre-push"
        }
    },
    "dependencies": {
        "@popperjs/core": "^2.9.2",
        "@styled-system/css": "^5.1.5",
        "change-case": "^4.1.2",
        "framer-motion": "^4.1.13",
        "lodash": "^4.17.21",
        "react-animate-height": "^2.0.23",
        "react-popper": "^2.2.5",
        "styled-system": "^5.1.5",
        "zustand": "^3.5.1"
    },
    "peerDependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "styled-components": "^5.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/preset-env": "^7.14.1",
        "@babel/preset-typescript": "^7.13.0",
        "@mapbox/hast-util-to-jsx": "^1.0.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^19.0.0",
        "@rollup/plugin-node-resolve": "^13.0.0",
        "@storybook/addon-essentials": "^6.2.9",
        "@storybook/addon-links": "^6.2.9",
        "@storybook/react": "^6.2.9",
        "@types/jest": "^26.0.23",
        "@types/lodash": "^4.14.168",
        "@types/node": "^15.0.2",
        "@types/react": "^17.0.5",
        "@types/react-dom": "^17.0.3",
        "@types/styled-components": "^5.1.9",
        "@types/styled-system": "^5.1.11",
        "@types/styled-system__css": "^5.0.15",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "babel-loader": "^8.2.2",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-functional": "^3.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-json": "^3.0.0",
        "eslint-plugin-lodash": "^7.2.0",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.23.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "file-loader": "^6.2.0",
        "husky": "6.0.0",
        "minimist": "^1.2.5",
        "mkdirp": "^1.0.4",
        "prettier": "^2.2.1",
        "react": "^17.0.2",
        "react-docgen-typescript": "^1.22.0",
        "react-dom": "^17.0.2",
        "rollup": "^2.47.0",
        "rollup-plugin-peer-deps-external": "^2.2.4",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "storybook-dark-mode": "^1.0.8",
        "svg-parser": "^2.0.4",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4",
        "unist-util-find": "^1.0.2"
    }
}
